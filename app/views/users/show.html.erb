<% provide(:title, @user.username) %>

<body>
<div id="create-post-modal" class="create-post-modal">
  <div class="create-post-modal-content"><span class="create-post-modal-close-button">&times;</span>

    <h2>Create a new post</h2>
    <div class="toggle-buttons">
      <button class="btn active" id="write-post-btn" type="button">Writepost</button>
      <button class="btn" id="photo-post-btn" type="button">Photopost</button>
    </div>
    <form id="write-post-form" class="form active">
      <div class="login-input-div">
        <label class="form-label login-input-label" style="margin: 6px -135px;position: absolute;color: #64748B;font-size: 12px;">What
          you say</label><textarea class="form-control post-what-you-say-ta"></textarea></div>
      <button class="btn btn-primary" type="submit">Share</button>
    </form>
    <form id="photo-post-form" class="form">
      <div class="custom-file-upload"><label class="form-label" id="file-name" for="choose-file-input">Select
        Photo</label><input class="form-control" type="file" id="choose-file-input"></div>
      <div class="login-input-div">
        <label class="form-label login-input-label" style="margin: 6px -135px;position: absolute;color: #64748B;font-size: 12px;">What
          you say</label><textarea class="form-control post-what-you-say-ta"></textarea></div>
      <button class="btn btn-primary" type="submit">Share</button>
    </form>
  </div>
</div>


<div id="create-post-button">
  <svg id="create-post-button-svg" xmlns="http://www.w3.org/2000/svg" width="70" height="70" viewBox="0 0 70 70" fill="none">
    <path fill-rule="evenodd" clip-rule="evenodd" d="M49.8936 0C60.9981 0 70 9.00193 70 20.1064V49.8936C70 60.9981 60.9981 70 49.8936 70H20.1064C9.00193 70 0 60.9981 0 49.8936V20.1064C0 9.00193 9.00193 0 20.1064 0H49.8936ZM49.8936 4.46809H20.1064C11.4696 4.46809 4.46809 11.4696 4.46809 20.1064V49.8936C4.46809 58.5304 11.4696 65.5319 20.1064 65.5319H49.8936C58.5304 65.5319 65.5319 58.5304 65.5319 49.8936V20.1064C65.5319 11.4696 58.5304 4.46809 49.8936 4.46809ZM37.2136 16.8245C37.0657 15.7341 36.131 14.8936 35 14.8936L34.6969 14.914L34.4061 14.9734C33.4605 15.2335 32.766 16.0995 32.766 17.1277V32.763L17.1277 32.766L16.8245 32.7864C15.7341 32.9343 14.8936 33.869 14.8936 35L14.914 35.3031C15.0619 36.3936 15.9967 37.234 17.1277 37.234L32.766 37.2311V52.8723L32.7864 53.1755C32.9343 54.2659 33.869 55.1064 35 55.1064L35.3031 55.086C36.3936 54.9381 37.234 54.0033 37.234 52.8723V37.234H52.8723L53.1755 37.2136C54.2659 37.0657 55.1064 36.131 55.1064 35L55.086 34.6969C54.9381 33.6064 54.0033 32.766 52.8723 32.766H37.234V17.1277L37.2136 16.8245Z"/>
  </svg>
</div>


<div class="container" id="content-container">
  <div class="row">
    <div class="col-md-8">
      <div id="content-container-row1" class="row">
        <div class="col-md-auto">
          <div>
            <% if @user.avatar.attached? %>
              <%= image_tag @user.avatar.variant(resize_to_limit: [160, 160]), class: "rounded-circle" %>
            <% else %>
              <%= image_tag "default-avatar.png", class: "rounded-circle", size: "160x160" %>
            <% end %>
          </div>
        </div>
        <div class="col">
          <div>
            <!-- Username & Follow Button -->
            <div id="profile-1">
              <p id="profile-username" style="font-family: 'Helvetica Neue';"> <%= @user.username %></p>
              <div id="profile-buttons">
                <div style="position: relative;">
                  <% if user_signed_in? && current_user != @user %>
                    <% if current_user.following.include?(@user) %>
                      <%= button_to 'Takipten Çık', unfollow_user_path(@user.username), method: :delete, class: "btn btn-warning" %>
                    <% else %>
                      <%= button_to 'Takip Et', follow_user_path(@user.username), method: :post, class: "btn btn-success" %>
                    <% end %>
                  <% end %>
                  
                  <button class="btn btn-primary follow-btn-dropdown" type="button">
                    <i class="fas fa-sort-down" style="font-size: 16px;"></i>
                  </button>

                  <button id="settings-button" class="svg-btn">
                    <svg xmlns="http://www.w3.org/2000/svg" width="21" height="4" viewBox="0 0 21 4" fill="none">
                      <ellipse cx="2.1" cy="2" rx="2.1" ry="2" fill="#262626"/>
                      <ellipse cx="10.5" cy="2" rx="2.1" ry="2" fill="#262626"/>
                      <ellipse cx="18.8999" cy="2" rx="2.1" ry="2" fill="#262626"/>
                    </svg>
                  </button>
                  <div id="dropdown-menu" class="hidden">
                    <ul>
                      <li><a href="<%= edit_user_path(@user.username) %>">Edit User Settings</a></li>
                      <li><a href="<%= edit_profile_path(@user.username) %>">Edit Profile</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>

            <!-- Profile Stats -->
            <div id="profile-stats">
              <p><span><%= @total_posts %></span>&nbsp;posts</p>
              <p><%= link_to followers_user_path(@user.username), class: "text-decoration-none text-dark" do %>
                <span><%= @user.followers.count %></span>&nbsp;followers
              <% end %></p>
              <p><%= link_to following_user_path(@user.username), class: "text-decoration-none text-dark" do %>
                <span><%= @user.following.count %></span>&nbsp;following
              <% end %></p>
            </div>

            <!-- Profile Info -->
            <div id="profile-name">
              <p id="profile-name-main"><%= @user.profile.name %> <%= @user.profile.surname %></p>
              <p id="profile-name-desc"><%= @user.profile.bio %></p>
            </div>
          </div>
        </div>
      </div>

      <!-- Moments -->
      <div id="story-box">
        <div class="story-item">
          <svg xmlns="http://www.w3.org/2000/svg" width="86" height="86" viewBox="0 0 86 86" fill="none">
            <circle cx="43" cy="43" r="42.5" stroke="#DBDBDB"/>
          </svg>
          <p>ACU</p>
        </div>
        <div class="story-item">
          <svg xmlns="http://www.w3.org/2000/svg" width="86" height="86" viewBox="0 0 86 86" fill="none">
            <circle cx="43" cy="43" r="42.5" stroke="#DBDBDB"/>
          </svg>
          <p>ACU</p>
        </div>
        <div class="story-item">
          <svg xmlns="http://www.w3.org/2000/svg" width="86" height="86" viewBox="0 0 86 86" fill="none">
            <circle cx="43" cy="43" r="42.5" stroke="#DBDBDB"/>
          </svg>
          <p>ACU</p>
        </div>
        <div class="story-item">
          <svg xmlns="http://www.w3.org/2000/svg" width="86" height="86" viewBox="0 0 86 86" fill="none">
            <circle cx="43" cy="43" r="42.5" stroke="#DBDBDB"/>
          </svg>
          <p>ACU</p>
        </div>
      </div>

      <!-- Section Buttons -->
      <div id="profile-section-buttons">
