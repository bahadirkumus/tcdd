<div class="container" id="content-container">
  <div class="row">
    <div class="col-md-8">
      <div id="content-container-row1" class="row">
        <div class="col-md-auto">
          <div id="profile-photo"></div>
        </div>
        <div class="col">
          <div>
            <div id="profile-1">
              <p id="profile-username" style="font-family: 'Helvetica Neue';"><%= "#{@user.username}" %></p>
              <div id="profile-buttons"><button class="btn btn-primary follow-btn" type="button">Follow</button><button class="btn btn-primary follow-btn-dropdown" type="button"><i class="fas fa-sort-down" style="font-size: 16px;"></i></button><svg xmlns="http://www.w3.org/2000/svg" width="21" height="4" viewBox="0 0 21 4" fill="none">
                <ellipse cx="2.1" cy="2" rx="2.1" ry="2" fill="#262626"/>
                <ellipse cx="10.5" cy="2" rx="2.1" ry="2" fill="#262626"/>
                <ellipse cx="18.8999" cy="2" rx="2.1" ry="2" fill="#262626"/>
              </svg></div>
            </div>
            <div id="profile-stats">
              <p><span>10</span>&nbsp;posts</p>
              <p><span>137</span>&nbsp;followers</p>
              <p><span>76</span>&nbsp;following</p>
            </div>
            <div id="profile-name">
              <p id="profile-name-main"><%= "#{@user.name} #{@user.surname}" %></p>
              <p id="profile-name-desc"><%= "#{@user.bio}" %></p>
            </div>
          </div>
        </div>
      </div>
